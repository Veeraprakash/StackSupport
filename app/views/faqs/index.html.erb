<DIV style="position:absolute; left:200px; top:200px;; overflow:visible">
<h1>FAQs</h1>
<h3>List of FAQs</h3>

<dd><table border = "1", style="width:75px; height:25px">
  <tr>

   <th>Title</th>
    <th>Category</th>
    <th>Status</th>
    <th>Votes Up</th>
    <th>Votes Down</th>
    <th>Options</th>
    <th></th>
  </tr>

<% @faqs.each do |faq| %>
  <tr>
   
    <td width="50px"><%= link_to faq.subject, faq %></td>
    <td><%= faq.category %></td>
    <td>
	<%= faq.private ? 'private' : 'public' %>
		
    </td>
    <td><%= faq.votes_up %></td>
    <td><%= faq.votes_down %></td>
   
    <td><%= link_to 'Edit', edit_faq_path(faq) %></td>
    <td><%= link_to 'Delete', faq, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

</DIV>
